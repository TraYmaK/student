sc_agent_for_searching_developer_projects
=> nrel_main_idtf:
	[sc-агент поиска проектов заданного разработчика]
    (*
        <- lang_ru;;
    *);
	[sc-agent for searching projects of specified developer]
    (* 
        <- lang_en;;
    *);
<- abstract_sc_agent;
=> nrel_primary_initiation_condition: 
	(sc_event_after_generate_outgoing_arc => action_initiated);
=> nrel_initiation_condition_and_result: 
	(..sc_agent_for_searching_developer_projects_condition => ..sc_agent_for_searching_developer_projects_result);
=> nrel_sc_agent_action_class:
	action_search_developer_projects;
<= nrel_sc_agent_key_sc_elements: 
{
	action_initiated;
	action_search_developer_projects
};
=> nrel_inclusion: 
	.platform_independent_implementation_of_sc_agent_for_searching_developer_projects
	(*
		<- platform_independent_abstract_sc_agent;;
		<= nrel_sc_agent_program: 
		{
			agent_program_for_searching_developer_projects;
			program_for_searching_developer_projects
		};;

		-> scp_agent_for_searching_developer_projects
		(*
			<- active_sc_agent;;
		*);;  
	*);;

..sc_agent_for_searching_developer_projects_condition
= [*
	action_search_developer_projects _-> .._action;;
	action_initiated _-> .._action;;
	.._action _-> rrel_1:: .._developer;;
*];;

..sc_agent_for_searching_developer_projects_result
= [*
	concept_mobile_project _-> .._project;;
*];;
